{
  "name": "vot-backend",
  "version": "1.2.0",
  "scripts": {
    "dev": "NODE_ENV=development bun run --watch src/index.ts",
    "start": "NODE_ENV=production bun src/index.ts",
    "migrate": "MIGRATOR_ACTION=upgrade bun run src/database/migrator.ts",
    "migrate:up": "bun run migrate",
    "migrate:down": "MIGRATOR_ACTION=downgrade bun run src/database/migrator.ts",
    "s3:disable-cors": "bun scripts/disable-cors.ts",
    "lint": "bunx oxlint --ignore-path=.oxlintignore && bunx eslint",
    "start:docker": "bun run migrate && bun run start",
    "prepare": "husky"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.723.0",
    "@aws-sdk/s3-request-presigner": "^3.723.0",
    "@elysiajs/swagger": "^1.2.0",
    "@smithy/fetch-http-handler": "^5.0.0",
    "@vot.js/core": "^2.1.6",
    "@vot.js/node": "^2.1.6",
    "@vot.js/shared": "^2.1.6",
    "bullmq": "^5.34.8",
    "elysia": "1.2.6",
    "elysia-http-status-code": "^1.0.9",
    "ioredis": "^5.4.2",
    "kysely": "^0.27.5",
    "pg": "^8.13.1",
    "pino": "^9.6.0",
    "pino-loki": "^2.4.0",
    "uuid": "^11.0.4"
  },
  "devDependencies": {
    "@sinclair/typebox": "^0.34.13",
    "@toil/translate": "^1.0.4",
    "@types/eslint__js": "^8.42.3",
    "@types/pg": "^8.11.10",
    "@types/uuid": "^10.0.0",
    "bun-types": "latest",
    "eslint": "^9.17.0",
    "eslint-plugin-oxlint": "^0.15.5",
    "husky": "^9.1.7",
    "oxlint": "^0.15.5",
    "pino-pretty": "^13.0.0",
    "typescript-eslint": "^8.19.1"
  },
  "module": "src/index.ts",
  "bun-create": {
    "start": "bun run src/index.ts"
  },
  "type": "module",
  "peerDependencies": {
    "typescript": "^5.4.3"
  }
}
