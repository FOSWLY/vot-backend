{
  "name": "vot-backend",
  "version": "1.0.0",
  "scripts": {
    "dev": "NODE_ENV=development bun run --watch src/index.ts",
    "start": "NODE_ENV=production bun src/index.ts",
    "migrate": "MIGRATOR_ACTION=upgrade bun run src/database/migrator.ts",
    "migrate:up": "bun run migrate",
    "migrate:down": "MIGRATOR_ACTION=downgrade bun run src/database/migrator.ts",
    "s3:disable-cors": "bun scripts/disable-cors.ts",
    "lint": "bunx oxlint --ignore-path=.oxlintignore && bunx eslint",
    "start:docker": "bun run migrate && bun run start",
    "prepare": "husky"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.629.0",
    "@aws-sdk/s3-request-presigner": "^3.629.0",
    "@elysiajs/swagger": "^1.1.1",
    "@smithy/fetch-http-handler": "^3.2.4",
    "bullmq": "^5.12.6",
    "elysia": "^1.1.6",
    "elysia-http-status-code": "^1.0.9",
    "ioredis": "^5.4.1",
    "kysely": "^0.27.4",
    "pg": "^8.12.0",
    "pino": "^9.3.2",
    "pino-loki": "^2.3.0",
    "uuid": "^10.0.0",
    "vot.js": "1.0.3"
  },
  "devDependencies": {
    "@types/eslint__js": "^8.42.3",
    "@types/pg": "^8.11.6",
    "@types/uuid": "^10.0.0",
    "bun-types": "latest",
    "eslint": "^9.9.0",
    "eslint-plugin-oxlint": "0.7.1",
    "eslint-plugin-sonarjs": "^1.0.4",
    "husky": "^9.1.4",
    "oxlint": "0.7.0",
    "pino-pretty": "^11.2.2",
    "typescript-eslint": "8.0.0"
  },
  "module": "src/index.ts",
  "bun-create": {
    "start": "bun run src/index.ts"
  },
  "type": "module",
  "peerDependencies": {
    "typescript": "^5.4.3"
  }
}
