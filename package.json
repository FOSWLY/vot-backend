{
  "name": "vot-backend",
  "version": "1.0.50",
  "scripts": {
    "dev": "NODE_ENV=development bun run --watch src/index.ts",
    "start": "NODE_ENV=production bun src/index.ts",
    "migrate": "MIGRATOR_ACTION=upgrade bun run src/database/migrator.ts",
    "migrate:up": "bun run migrate",
    "migrate:down": "MIGRATOR_ACTION=downgrade bun run src/database/migrator.ts",
    "prepare": "husky"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.598.0",
    "@aws-sdk/lib-storage": "^3.598.0",
    "@bogeychan/elysia-logger": "^0.0.21",
    "@elysiajs/static": "^1.0.3",
    "@elysiajs/swagger": "^1.0.4",
    "bullmq": "^5.8.1",
    "elysia": "^1.0.15",
    "elysia-http-status-code": "^1.0.9",
    "kysely": "^0.27.3",
    "pg": "^8.12.0",
    "pino-pretty": "^11.0.0",
    "uuid": "^10.0.0"
  },
  "devDependencies": {
    "@types/pg": "^8.11.6",
    "@types/uuid": "^9.0.8",
    "@typescript-eslint/eslint-plugin": "^7.8.0",
    "@typescript-eslint/parser": "^7.8.0",
    "bun-types": "latest",
    "eslint": "^8.57.0",
    "eslint-plugin-oxlint": "^0.2.4",
    "eslint-plugin-sonarjs": "^0.25.1",
    "husky": "^9.0.11",
    "oxlint": "^0.2.14"
  },
  "module": "src/index.js",
  "bun-create": {
    "start": "bun run src/index.ts"
  },
  "type": "module",
  "peerDependencies": {
    "typescript": "^5.0.0"
  }
}
